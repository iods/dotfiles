#!/bin/bash
#
#
#

# check for and upgrade any formula updates
sleep 1
brew upgrade

# install GNU core utilities (OSX ones are outdated)
# echo "Donâ€™t forget to add $(brew --prefix coreutils)/libexec/gnubin to \$PATH."
# install GNU `find`, `locate`, `updatedb`, and `xargs` g-prefixed
brew install \
     coreutils \
     findutils \
     bash \
     wget \
     git \
     rename \
     tree \
     dockutil

# upgrade any of the cask applications
brew upgrade --cask

# remove outdated versions from the cellar
brew cleanup

